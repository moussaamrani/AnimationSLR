@Article{10.1007/s11042-023-15950-5,
  author       = {Haigen Yang and Zhun Xia and Yanqing Chen and Linqun Zhu and Luohao Dai and Ruotian Xu and GuiYing Sun and Hongyang Yu and Wenting Xu},
  date         = {2024-02},
  journaltitle = {Multimedia Tools and Applications},
  title        = {Research on visual simulation for complex weapon equipment interoperability based on MBSE},
