@Article{10.1186/s12874-024-02149-x,
  author       = {Sharon Jepkorir Sawe and Richard Mugo and Marta Wilson-Barthes and Brianna Osetinsky and Stavroula A. Chrysanthopoulou and Faith Yego and Ann Mwangi and Omar Galárraga},
  date         = {2024-01},
  journaltitle = {BMC Medical Research Methodology},
  title        = {Gaussian process emulation to improve efficiency of computationally intensive multidisease models: a practical tutorial with adaptable R code},
  doi          = {10.1186/s12874-024-02149-x},
