@Article{10.1007/s12665-024-11461-0,
  author       = {Diego Portalanza and Vanesa C. Pántano and Cristian Felipe Zuluaga and Marcos Roberto Benso and Arturo Corrales Suastegui and Natalia Castillo and Silvina Solman},
  date         = {2024-03},
  journaltitle = {Environmental Earth Sciences},
  title        = {Can extreme climatic and bioclimatic indices reproduce soy and maize yields in Latin America? Part 1: an observational and modeling perspective},
