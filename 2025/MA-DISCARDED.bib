@Article{10.1007/s11227-024-05951-y,
  author       = {Sensen Wang and Wenjun Liu and Shuaibin Yang and Hui Huang},
  date         = {2024-06},
  journaltitle = {The Journal of Supercomputing},
  title        = {An optimized AdaBoost algorithm with atherosclerosis diagnostic applications: adaptive weight-adjustable boosting},
