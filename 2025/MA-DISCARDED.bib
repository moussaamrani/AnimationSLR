@InProceedings{10.1007/978-3-031-09843-7_8,
  author       = {Reiko Heckel and Zobia Erum and Nitia Rahmi and Albert Pul},
  booktitle    = {Graph Transformation},
  date         = {2022-01},
  title        = {Visual Smart Contracts for DAML},
  doi          = {10.1007/978-3-031-09843-7_8},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-031-09843-7_8},
  abstract     = {The Digital Asset Modelling Language (DAML) enables low-code development of smart contract applications. Starting from a high-level but textual notation, DAML thus implements the lower end of a model-driven development process, from a platform-specific level to implementations on a range of blockchain platforms. Existing approaches for modelling smart contracts support a domain-oriented, conceptual view but do not link to the same technology-specific level. We develop a notation based on class diagrams and visual contracts that map directly to DAML smart contracts. The approach is grounded in an operational semantics in terms of graph transformation that accounts for the more complex behavioural features of DAML, such as its role-based access control and the order of contract execution and archival. The models, with their mappings to DAML and their operational semantics, are introduced via the Doodle case study from a DAML tutorial and validated through testing the graph transformation system against the DAML code using the Groove model checker.},
  creationdate = {2025-08-08T16:56:28},
  keywords     = {smart contracts, DAML, model-based development, UML, visual contracts, graph transformation, Groove},
  owner        = {mamrani},
}


@InProceedings{10.1007/978-3-031-61003-5_8,
