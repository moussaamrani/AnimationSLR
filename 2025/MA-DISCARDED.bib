  author       = {Sunil Sharma and Prabir Sarkar},
  date         = {2024-10},
  journaltitle = {International Journal on Interactive Design and Manufacturing (IJIDeM)},
  title        = {A framework to describe biological entities for bioinspiration},
  doi          = {10.1007/s12008-023-01281-0},
