@Article{10.1007/s11269-022-03420-w,
  author       = {Miloš Milašinović and Damjan Ivetić and Milan Stojković and Dragan Savić},
  date         = {2023-02},
  journaltitle = {Water Resources Management},
  title        = {Failure Conditions Assessment of Complex Water Systems Using Fuzzy Logic},
