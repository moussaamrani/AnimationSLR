@Article{10.1007/s42835-023-01524-w,
  author       = {Hai-Lin Feng and An-Ke Xu},
  date         = {2024-01},
  journaltitle = {Journal of Electrical Engineering Technology},
  title        = {A Data Compensation Model for Predicting SOH and RUL of Lithium–Ion Battery},
  doi          = {10.1007/s42835-023-01524-w},
  number       = {1},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s42835-023-01524-w},
  volume       = {19},
  abstract     = {Accurately estimating the state of health and remaining useful life of lithium–ion batteries is particularly critical to ensure safety and reliability. In this paper, ES-EDM-DCM, a data compensation model which fusing a new empirical model ES-EDM and a feature-driven model is established. The ES-EDM describes the degradation trajectory of li–ion batteries better. The feature driven model is established based on support vector regression which driven by a single health feature extracted and optimized from partial battery data. Finally, two datasets are used to verify that the prediction error of the proposed model is reduced by 0.1–3.68},
  creationdate = {2025-08-08T17:06:00},
  keywords     = {Lithium–ion battery, State of health, Remaining useful life, Data compensation, Empirical degradation model},
