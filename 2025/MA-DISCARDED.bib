@Article{10.1007/s10489-022-03297-7,
  author       = {Zihua Chen and Chuanli Wang and Jingzhao Li and Shunxiang Zhang and Qichun Ouyang},
  date         = {2022-11},
  journaltitle = {Applied Intelligence},
