  author       = {Ashutosh Kumar Singh and Shashank Srivastava and Shashwati Banerjea},
  date         = {2023-09},
  journaltitle = {Journal of Ambient Intelligence and Humanized Computing},
  title        = {Evaluating heuristic techniques as a solution of controller placement problem in SDN},
