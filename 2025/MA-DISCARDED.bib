@Article{10.1007/s00449-023-02913-1,
  author       = {Kabilan Subash Chandra Bose and Mohd Imran Shah and Jayachandran Krishna and Meenakshisundaram Sankaranarayanan},
  date         = {2023-10},
  journaltitle = {Bioprocess and Biosystems Engineering},
  title        = {Genome-scale metabolic model analysis of Pichia pastoris for enhancing the production of S-adenosyl-l-methionine},
