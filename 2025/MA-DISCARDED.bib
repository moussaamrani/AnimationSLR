@InProceedings{10.1007/978-3-031-66326-0_6,
  author       = {Giordano d’Aloisio and Andrea D’Angelo and Francesca Marzi and Diana Marco and Giovanni Stilo and Antinisca Marco},
  booktitle    = {Software Architecture. ECSA 2023 Tracks, Workshops, and Doctoral Symposium},
  date         = {2024-01},
  title        = {Data-Driven Analysis of Gender Fairness in the Software Engineering Academic Landscape},
