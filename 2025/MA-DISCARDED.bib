@InProceedings{10.1007/978-981-97-3994-3_15,
  author       = {Lomesh S. Mahajan and Monali Kirange and Ganesh Suryawanshi},
  booktitle    = {Innovation in Smart and Sustainable Infrastructure, Volume 2},
  date         = {2024-01},
  title        = {An Artificial Intelligence Applications in Structural Engineering},
  doi          = {10.1007/978-981-97-3994-3_15},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-97-3994-3_15},
  abstract     = {Artificial intelligence (AI) is a computerised technique to solving complex engineering problems that tries to imitate human-like thinking/cognition skills. AI is well suited to engineering situations with a large number of inputs. Traditional modelling and statistical approaches can be replaced by AI. In the realm of structural engineering, experimentation is a herculean endeavour; hence, AI-based algorithms are feasible options for predicting different engineering design parameters such as structural reaction and compressive strength. The purpose of this study is to review the many artificial intelligence applications in structural engineering that have arisen in recent years. The importance of AI in the structural engineering sector is examined after a brief introduction to AI is given. Following that, a survey of contemporary uses of AI techniques like as ML, PR, and DL in structural engineering is offered, along with an examination of their capacity to satisfy the limits of traditional models. This study looks at current and future AI applications in structural engineering, as well as the opportunities and challenges that may arise if AI technologies are effectively used in structural engineering practice.},
