@Article{10.1038/s41551-021-00789-8,
  author       = {Rumana Rashid and Yu-An Chen and John Hoffer and Jeremy L. Muhlich and Jia-Ren Lin and Robert Krueger and Hanspeter Pfister and Richard Mitchell and Sandro Santagata and Peter K. Sorger},
  date         = {2022-05},
  journaltitle = {Nature Biomedical Engineering},
  title        = {Narrative online guides for the interpretation of digital-pathology images and tissue-atlas data},
  doi          = {10.1038/s41551-021-00789-8},
  number       = {5},
  url          = {https://www.nature.com/articles/s41551-021-00789-8.pdf},
  volume       = {6},
  abstract     = {Multiplexed tissue imaging facilitates the diagnosis and understanding of complex disease traits. However, the analysis of such digital images heavily relies on the experience of anatomical pathologists for the review, annotation and description of tissue features. In addition, the wider use of data from tissue atlases in basic and translational research and in classrooms would benefit from software that facilitates the easy visualization and sharing of the images and the results of their analyses. In this Perspective, we describe the ecosystem of software available for the analysis of tissue images and discuss the need for interactive online guides that help histopathologists make complex images comprehensible to non-specialists. We illustrate this idea via a software interface (Minerva), accessible via web browsers, that integrates multi-omic and tissue-atlas features. We argue that such interactive narrative guides can effectively disseminate digital histology data and aid their interpretation. This Perspective describes the ecosystem of software available for the analysis of tissue images and discusses the need for interactive online guides that help histopathologists make complex images understandable to non-specialists.},
  creationdate = {2025-08-08T17:10:25},
  owner        = {mamrani},
  page         = {515–526},
  publisher    = {Nature},
}


