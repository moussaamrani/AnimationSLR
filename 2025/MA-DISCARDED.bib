  booktitle    = {Evolutionary Multi-Criterion Optimization},
  date         = {2023-01},
  title        = {An Improved Fuzzy Classifier-Based Evolutionary Algorithm for Expensive Multiobjective Optimization Problems with Complicated Pareto Sets},
