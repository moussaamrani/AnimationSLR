@InProceedings{10.1007/978-3-031-77095-1_2,
  author       = {Sheng He and Qinglin Huang and Shaoshuai Jiao and Zepeng Lin and Jinxuan Lin and Jun Ren and Dengbin Xiong and Liang-Jie Zhang},
  booktitle    = {Blockchain – ICBC 2024},
  date         = {2025-01},
  title        = {Accelerating Blockchain Application Development: Integrating Blockchain as a Service Within Low-Code Platforms},
  doi          = {10.1007/978-3-031-77095-1_2},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-031-77095-1_2},
  abstract     = {The integration of Blockchain as a Service (BaaS) within the Low-Code Development Platforms (LCDPs) is expected to revolutionize the speed and accessibility of blockchain application development. This paper explores the synergy between BaaS and the low-code or no-code development environments in Platform as a Service (PaaS) frameworks, examining opportunities, challenges, and implications for developers and organizations. LCDPs have gained popularity for simplifying application creation, empowering a broad spectrum of developers from citizen developers to experienced professionals. Concurrently, blockchain technology offers enhanced security and transparency but traditionally requires specialized skills for implementation. By embedding BaaS capabilities within LCDPs, developers can leverage blockchain benefits without extensive expertise, streamlining development processes and accelerating time-to-market for blockchain applications. Additionally, it addresses challenges related to security, scalability, and the complexity of managing blockchain operations within simplified development environments. By presenting best practices and identifying opportunities, this paper aims to contribute to the advancement of accessible and efficient blockchain application development.},
  creationdate = {2025-08-08T17:34:28},
  keywords     = {Blockchain as a Service, Low-Code Development Platform, Platform as a Service, Decentralized Application},
  owner        = {mamrani},
}


@InProceedings{10.1007/978-3-031-68905-5_3,
  author       = {B. Kamala and D. Harini and A. Akshaya Selvi},
  booktitle    = {Deep Sciences for Computing and Communications},
  date         = {2024-01},
  title        = {Digital Identity System for Real World Asset Using Blockchain},
  doi          = {10.1007/978-3-031-68905-5_3},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-031-68905-5_3},
  abstract     = {The ability to develop digital twins (DTs) has been made by the quick development of computing, storage and networking technology. A representation of a component, object or piece of machinery from the real world is known as the digital replica that is a Digital Twin (DT). It ensures secure and traceability, accessibility and immutable transaction and data provenance, we develop Dts using an approach based on blockchain. In this method smart contracts are used to track and save the actions of those involved in DT creation. The digital twins for costly gold jewelry is the method that has been used. The possibility of priceless jewelry being stolen or lost is very high. So, we came up with the concept of developing a distinct NFT for each item. As a result, each piece of jewelry will contain a special NFT token that will be linked to the original piece. Non-fungible tokens (NFTs) are tokens that exist on a blockchain and cannot be duplicated. So only if anybody owns the NFT, the jewelry belongs to them. Even if the jewelry is lost or stolen, a public address will be needed to sell the jewelry so nobody can’t sell it without the jewelry owner’s knowledge. In every piece of jewelry, the NFT will be engraved into the jewelry and also an Ethereum wallet will be created and a public address will also be allocated to the jewelry owner. When person A is selling a piece of jewelry to person B, all the NFT ownership has to be transferred to person B. This system is secured because the NFT Ownership is something which can’t be stolen. After all, it is not a real asset.},
  creationdate = {2025-08-08T17:34:28},
  keywords     = {Digital Twin (DT), Non-Fungible Tokens (NFT), NFT Ownership, Real World asset-jewelry, Tracking},
  owner        = {mamrani},
}


@InProceedings{10.1007/978-981-96-6243-2_9,
  author       = {Qishun Ma and Haoyu Gao and Xiang Xu and Zhiwei Liu and Tiankai Xu and Hong Lei},
  booktitle    = {Blockchain Technology and Application},
  date         = {2025-01},
  title        = {DEDF: A Dual-Engine-Driven Framework for Blockchain Ecosystem Security Regulation},
  doi          = {10.1007/978-981-96-6243-2_9},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-96-6243-2_9},
  abstract     = {With the evolution of blockchain technology from versions 1.0 to 3.0, its application domain has expanded from a single payment scenario to multiple areas, forming a complex blockchain ecosystem. However, the security issues of this ecosystem are becoming increasingly prominent, necessitating regulation. On the basis of the analysis and summary of the composition of the blockchain ecosystem and its security challenges, we propose a dual-engine-driven framework (DEDF) for blockchain ecosystem security regulation, which conducts security regulations from both business compliance supervision and technical risk assessment. The experimental results show that the proposed framework can efficiently deploy regulatory rules, take effect, and analyze blockchain security risks in a timely manner, meeting security regulation needs under various user scales and application scenarios. In short, we provide a high-level framework for blockchain ecosystem regulation for academia and industry.},
  creationdate = {2025-08-08T17:34:29},
  keywords     = {blockchain security, blockchain ecosystem, security regulation, cybersecurity},
  owner        = {mamrani},
}


@InProceedings{10.1007/978-3-030-94335-6_18,
  author       = {Tahani Abu Musa and Abdelaziz Bouras},
  booktitle    = {Product Lifecycle Management. Green and Blue Technologies to Support Smart and Sustainable Organizations},
  date         = {2022-01},
  title        = {Anomaly Detection in Blockchain-Enabled Supply Chain: An Ontological Approach},
  doi          = {10.1007/978-3-030-94335-6_18},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-030-94335-6_18},
  abstract     = {In our work, we propose an anomaly detection framework, for detecting anomalous transactions in business processes from transaction event logs. Such a framework will help enhance the accuracy of anomaly detection in the global Supply Chain, improve the multi-level business processes workflow in the Supply Chain domain, and will optimize the processes in the Supply Chain in terms of security and automation. In the proposed work, Ontology is utilized to provide anomaly classification in business transactions, based on crafted SWRL rules for that purpose. Our work has been evaluated based on logs generated from simulating a generic business process model related to a procurement scenario, and the findings show that our framework was able to detect and classify anomalous transactions form those logs.},
  creationdate = {2025-08-08T17:34:29},
  keywords     = {Anomaly detection, Business process, Supply chain, Blockchain},
  owner        = {mamrani},
}


@InProceedings{10.1007/978-3-031-60433-1_20,
  author       = {Karl Seidenfad and Maximilian Greiner and Jan Biermann and David Dannenberg and Sven Keineke and Ulrike Lechner},
  booktitle    = {Innovations for Community Services},
