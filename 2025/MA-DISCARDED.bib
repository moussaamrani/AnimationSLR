@Article{10.1007/s10479-024-05875-z,
  author       = {Shahriar Akter and Saida Sultana and Angappa Gunasekaran and Ruwan J. Bandara and Shah J Miah},
  date         = {2024-02},
  journaltitle = {Annals of Operations Research},
  title        = {Tackling the global challenges using data-driven innovations},
