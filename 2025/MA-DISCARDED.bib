@Article{10.1007/s00158-025-03994-0,
  author       = {Chuwei Luo and Jiang Chen and Yi Liu and Hang Xiang},
  date         = {2025-03},
  journaltitle = {Structural and Multidisciplinary Optimization},
  title        = {Enhancing aerodynamic and aeroelastic performance of axial compressor through multi-degree-of-freedom parameterization and data-driven multidisciplinary optimization},
  doi          = {10.1007/s00158-025-03994-0},
  number       = {3},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s00158-025-03994-0},
  volume       = {68},
  abstract     = {Multidisciplinary design optimization (MDO) facilitates a comprehensive consideration of inter-disciplinary coupling, overcoming the lengthy processes of conventional serial design and charting a progressive trajectory for compressor optimization. However, the presence of multiple disciplines poses challenges concerning computational efforts and system organizational integration. This paper introduces a novel MDO system by integrating a directly manipulated free-form deformation (DFFD) method and a data-driven model with pre-screening strategy. The DFFD enables precise deformation of compressor surfaces with fewer control points to reduce variables, while data-driven models can significantly accelerate the convergence of complex multi-objective optimization problems. It is applied to an axial compressor to enhance its aerodynamic and aeroelastic performance. A fluid-domain mesh-based method is employed to construct the blade finite element model, addressing the fluid–structure interface interpolation. By progressively increasing the outlet pressure to search the compressor near surge point, the prediction accuracy of surge margin during the optimization process is effectively improved. Compared to the prototype, the isentropic efficiency of optimized compressor at the design point and peak increases by 1.22 and 0.8 percentage points, respectively, and the surge margin improves by 3.23},
  creationdate = {2025-08-08T17:10:13},
  keywords     = {Multidisciplinary design optimization (MDO), Data-driven, Directly manipulated free-form deformation (DFFD), Fluid–structure interpolation, Surge margin},
  owner        = {mamrani},
  page         = {1–22},
  publisher    = {Springer},
}


@Article{10.1007/s10270-023-01080-x,
  author       = {Gauthier Lyan and Jean-Marc Jézéquel and David Gross-Amblard and Romain Lefeuvre and Benoit Combemale},
  date         = {2023-10},
  journaltitle = {Software and Systems Modeling},
  title        = {Reasoning over time into models with DataTime},
  doi          = {10.1007/s10270-023-01080-x},
  number       = {5},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s10270-023-01080-x},
  volume       = {22},
  abstract     = {Models at runtime have been initially investigated for adaptive systems. Models are used as a reflective layer of the current state of the system to support the implementation of a feedback loop. More recently, models at runtime have also been identified as key for supporting the development of full-fledged digital twins. However, this use of models at runtime raises new challenges, such as the ability to seamlessly interact with the past, present, and future states of the system. In this paper, we propose a framework called DataTime to implement models at runtime which capture the state of the system according to the dimensions of both time and space, here modeled as a directed graph where both nodes and edges bear local states (i.e., values of properties of interest). DataTime offers a unifying interface to query the past, present, and future (predicted) states of the system. This unifying interface provides (i) an optimized structure of the time series that capture the past states of the system, possibly evolving over time, (ii) the ability to get the last available value provided by the system’s sensors, and (iii) a continuous micro-learning over graph edges of a predictive model to make it possible to query future states, either locally or more globally, thanks to a composition law. The framework has been developed and evaluated in the context of the Intelligent Public Transportation Systems of the city of Rennes (France). This experimentation has demonstrated how DataTime can be used for managing data from the past, the present, and the future and facilitate the development of digital twins.},
  creationdate = {2025-08-08T17:10:14},
  keywords     = {Models at runtime, Digital twins, Data analysis, Intelligent Public Transportation Systems},
  owner        = {mamrani},
  page         = {1689–1712},
  publisher    = {Springer},
}


