@Article{10.1007/s00170-024-13717-2,
  author       = {Zhilei Zhao and Hu Zhu},
  date         = {2024-08},
  journaltitle = {The International Journal of Advanced Manufacturing Technology},
  title        = {Research on the multi-stage CNC incremental forming of unequal height sheet metal part with large forming angle},
  doi          = {10.1007/s00170-024-13717-2},
  number       = {9-10},
