@Article{10.1038/s41598-024-83581-3,
  author       = {Xukuan Xu and Donghui Li and Jinghou Bi and Michael Moeckel},
  date         = {2024-12},
  journaltitle = {Scientific Reports},
  title        = {AutoML based workflow for design of experiments (DOE) selection and benchmarking data acquisition strategies with simulation models},
  doi          = {10.1038/s41598-024-83581-3},
  number       = {1},
  url          = {https://www.nature.com/articles/s41598-024-83581-3.pdf},
  volume       = {14},
  abstract     = {Design of experiments (DOE) is an established method to allocate resources for efficient parameter space exploration. Model based active learning (AL) data sampling strategies have shown potential for further optimization. This paper introduces a workflow for conducting DOE comparative studies using automated machine learning. Based on a practical definition of model complexity in the context of machine learning, the interplay of systematic data generation and model performance is examined considering various sources of uncertainty: this includes uncertainties caused by stochastic sampling strategies, imprecise data, suboptimal modeling, and model evaluation. Results obtained from electrical circuit models with varying complexity show that not all AL sampling strategies outperform conventional DOE strategies, depending on the available data volume, the complexity of the dataset, and data uncertainties. Trade-offs in resource allocation strategies, in particular between identical replication of data points for statistical noise reduction and broad sampling for maximum parameter space exploration, and their impact on subsequent machine learning analysis are systematically investigated. Results indicate that replication oriented strategies should not be dismissed but may prove advantageous for cases with non-negligible noise impact and intermediate resource availability. The provided workflow can be used to simulate practical experimental conditions for DOE testing and DOE selection.},
  creationdate = {2025-08-08T16:56:30},
  owner        = {mamrani},
  page         = {1–13},
  publisher    = {Nature},
}


@InProceedings{10.1007/978-981-96-2268-9_12,
  author       = {Beilei Yang and Junhong Zhang and Yuxiang Zhang},
  booktitle    = {Advances in Guidance, Navigation and Control},
  date         = {2025-01},
  title        = {UML-Based IVMS Redundancy Monitoring Modeling},
  doi          = {10.1007/978-981-96-2268-9_12},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-96-2268-9_12},
  abstract     = {In order to meet the requirements of high complexity and high safety in the design of redundancy monitoring function of integrated aircraft management system (IVMS), a UML-based modeling method is proposed to realize the requirement analysis of the system function in this paper. Both static model and dynamic model of IVMS redundancy monitoring are built via UML. The static model describes the integrated redundancy monitoring function scene, the running context, the abstract of the complex monitoring object class, and constructs the object activity logic, while the dynamic model describes the state change logic of the redundancy monitoring object in the running scene. Finally, based on the dynamic model, this paper constructs the redundancy monitoring state machine model and implements the function simulation through the SCADE Suit modeling tool.},
  creationdate = {2025-08-08T16:56:30},
