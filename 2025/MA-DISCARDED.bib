@InProceedings{10.1007/978-3-030-69288-9_4,
  author       = {Travis Herman Butler and Georgi Yordanov Georgiev},
  booktitle    = {Efficiency in Complex Systems},
  date         = {2022-01},
  title        = {Self-Organization in Stellar Evolution: Size-Complexity Rule},
  doi          = {10.1007/978-3-030-69288-9_4},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-030-69288-9_4},
  abstract     = {Complexity Theory is highly interdisciplinary, therefore, any regularities must hold on all levels of organization, independent on the nature of the system. An open question in science is how complex systems self-organize to produce emergent structures and properties, a branch of non-equilibrium thermodynamics. It has long been known that there is a quantity-quality transition in natural systems. This is to say that the properties of a system depend on its size. More recently, this has been termed the size-complexity rule, which means that to increase their size, systems must increase their complexity, and that to increase their complexity they must grow in size. This rule goes under different names in different disciplines and systems of different nature, such as the area-speciation rule, economies of scale, scaling relations (allometric) in biology and for cities, and many others. We apply the size-complexity rule to stars to compare them with other complex systems in order to find universal patterns of self-organization independent of the substrate. Here, as a measure of complexity of a star, we are using the degree of grouping of nucleons into atoms, which reduces nucleon entropy, increases the variety of elements, and changes the structure of the star. As seen in our previous work, complexity, using action efficiency, is in power law proportionality of all other characteristics of a complex system, including its size. Here we find that, as for the other systems studied, the complexity of stars is in a power law proportionality with their size—the bigger a system is, the higher its level of complexity is—despite differing explosion energies and initial metallicities from simulations and data, which confirms the size-complexity rule and our model.},
  creationdate = {2025-08-08T17:51:00},
  keywords     = {Self-organization, Stellar evolution, Size-complexity rule, Complex system, Principle of least action, Nucleosynthesis, Complexity theory, Quantity-quality transition},
  owner        = {mamrani},
}


@InProceedings{10.1007/978-981-99-1964-2_112,
  author       = {Ming-jun Cai and Feng-ming Lu and Jia-liang Zhang and Ri-su Hu and Dong-cheng Liu},
  booktitle    = {Proceedings of the International Field Exploration and Development Conference 2022},
  date         = {2023-01},
  title        = {High Precision Reservoir Description Technology in Complex Fault Block Oilfield},
  doi          = {10.1007/978-981-99-1964-2_112},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-99-1964-2_112},
  abstract     = {Complex Fault Block Reservoir (CFBR) is one of the main types of oil reservoir in the eastern oilfields of China, and it is in the stage of ultra-high water cut development. The description of Complex Fault Block Reservoir has aroused wild attention of experts and scholars. As the waterflood multiple of old oilfields increases, the physical properties of reservoirs change, and the distribution of remaining oil becomes more dispersed. Meanwhile, the potential of reservoirs is more vulnerable to the effects of small and micro terranes, reservoir heterogeneity and differential seepage field. Therefore, based on the fine reservoir description, a high-precision reservoir description technology is proposed in this article. By using the high precision reservoir description technology, not only the research objectives are more accurate, but also the ultra-high water cut characteristic of complex fault block oilfield is more highlighted in the research. In addition, the research methods are developing towards intelligence. High precision reservoir description technology is the continuation and elevation of fine reservoir description technology, which will play an important role in the deep development of old oilfields.},
  creationdate = {2025-08-08T17:51:00},
  keywords     = {High precision reservoir description, complex fault block, small and micro terrane, well seismic configuration phase, time-lapse earthquake, seepage field},
  owner        = {mamrani},
}


