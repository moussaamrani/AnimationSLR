}


@Article{10.1007/s12541-024-01139-9,
  author       = {Guo-hua Chen and Bo Zhou and Xiao Zhao and Zhi-yang Zhang and Qing Yan and Jie Mao},
  date         = {2025-03},
  journaltitle = {International Journal of Precision Engineering and Manufacturing},
  title        = {Study on Optimization Method for CNC Machining Plastic-Shaped Appliances Based on ICOA Algorithm},
