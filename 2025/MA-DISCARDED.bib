@Article{10.1007/s11081-021-09660-w,
  author       = {O. Nejadseyfi and H. J. M. Geijselaers and E. H. Atzema and M. Abspoel and A. H. Boogaard},
  date         = {2022-12},
  journaltitle = {Optimization and Engineering},
  title        = {Accounting for non-normal distribution of input variables and their correlations in robust optimization},
  doi          = {10.1007/s11081-021-09660-w},
  number       = {4},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s11081-021-09660-w},
  volume       = {23},
  abstract     = {In this work, metamodel-based robust optimization is performed using measured scatter of noise variables. Principal component analysis is used to describe the input noise using linearly uncorrelated principal components. Some of these principal components follow a normal probability distribution, others however deviate from a normal probability distribution. In that case, for more accurate description of material scatter, a multimodal distribution is used. An analytical method is implemented to propagate the noise distribution via metamodel and to calculate the statistics of the response accurately and efficiently. The robust optimization criterion as well as the constraints evaluation are adjusted to properly deal with multimodal response. Two problems are presented to show the effectiveness of the proposed approach and to validate the method. A basketball free throw in windy weather condition and forming of B-pillar component are presented. The significance of accounting for non-normal distribution of input variables using multimodal distributions is investigated. Moreover, analytical calculation of response statistics, and adjustment of the robust optimization problem are presented and discussed.},
  creationdate = {2025-08-08T17:28:41},
  keywords     = {Robust optimization, Multimodal input and output distribution, Principal component analysis, Coil-to-coil variation, B-pillar},
  owner        = {mamrani},
