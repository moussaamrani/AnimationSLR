@Article{10.1007/s42452-025-07304-7,
  author       = {Deepak Chaulagain and Ram Lakhan Ray and Abdulfatai Olatunji Yakub and Noel Ngando Same and Jaebum Park Jong and Wook Roh and Dongjun Suh and Jeong-Ok Lim and Jeung-Soo Huh},
  date         = {2025-07},
  journaltitle = {Discover Applied Sciences},
  title        = {Impacts of climate change on hydrological patterns and implications for hydroelectric power generation in Khimti River Basin, Nepal},
