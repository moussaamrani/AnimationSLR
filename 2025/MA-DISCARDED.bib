@Article{10.1007/s12652-021-03261-2,
  author       = {Cho Youn Lee and Jin Gon Shon and Ji Su Park},
  date         = {2022-10},
  journaltitle = {Journal of Ambient Intelligence and Humanized Computing},
  title        = {An edge detection–based eGAN model for connectivity in ambient intelligence environments},
  doi          = {10.1007/s12652-021-03261-2},
  number       = {10},
