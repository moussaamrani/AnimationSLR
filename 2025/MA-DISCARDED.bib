@Article{10.1007/s00607-023-01165-x,
  author       = {Manel Mena and Javier Criado and Luis Iribarne and Antonio Corral and Richard Chbeir and Yannis Manolopoulos},
  date         = {2023-08},
  journaltitle = {Computing},
  title        = {Towards high-availability cyber-physical systems using a microservice architecture},
  doi          = {10.1007/s00607-023-01165-x},
  number       = {8},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s00607-023-01165-x},
  volume       = {105},
  abstract     = {In the past few years the use of IoT devices has grown exponentially. When it comes to working with them, we find a series of problems that are not easy to solve. On the one hand, the simple fact of communicating with those devices can be problematic since they can use different types of technologies regarding that communication. On the other hand, these types of devices usually aim to perform their function using the lowest possible energy, meaning they have certain constraints in terms of performance. Trying to work with these devices in high-availability environments becomes difficult because of those restraints. This paper introduces digital dice, a virtual representation of IoT devices and cyber-physical systems based on microservices that uses the standard established by the W3C, the Web of Things, as the underlying framework to declare its possible interactions. The article puts forward the different strategies that Digital Dice uses to mitigate the problems raised. Furthermore, we contrast the performance of Digital Dice with using the devices directly, demonstrating its advantages in a process that requires High Availability.},
  creationdate = {2025-08-08T17:40:45},
  keywords     = {IoT, CPS, Microservices, WoT, High availability},
  owner        = {mamrani},
  page         = {1745–1768},
  publisher    = {Springer},
}


