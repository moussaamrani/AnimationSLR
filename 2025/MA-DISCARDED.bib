  title        = {A prototype adaptive mesh generator for enhancing computational efficiency and accuracy in physically-based modeling of flood-landslide hazards},
  doi          = {10.1016/j.envsoft.2025.106458},
  url          = {https://api.elsevier.com/content/abstract/scopus_id/105002132253},
