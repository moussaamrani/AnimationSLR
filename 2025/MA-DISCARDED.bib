  date         = {2024-08},
  journaltitle = {Circuits, Systems, and Signal Processing},
  title        = {Simple Structure Controller Design for Exponential Stabilization of Feedforward Nonlinear Time-Delay Systems},
  doi          = {10.1007/s00034-024-02690-6},
  number       = {8},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s00034-024-02690-6},
