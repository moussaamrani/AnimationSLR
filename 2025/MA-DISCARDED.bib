@InProceedings{10.1007/978-981-97-0922-9_52,
  author       = {Chulei Zhang and Ru Wang and Zixiang Hu and Wenzhi Wu and Jingdan Yu and Yan Yan},
  booktitle    = {Advances in Mechanical Design},
  date         = {2024-01},
  title        = {Prediction and Optimization of Residual Stress After Reflow Soldering of BGA Solder Joints Based on Multi-fidelity Surrogate Model},
  doi          = {10.1007/978-981-97-0922-9_52},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-97-0922-9_52},
  abstract     = {To solve the problem of difficult physical measurement and high simulation cost of residual stress after reflow soldering of BGA solder joints, a prediction and optimization method of the maximum residual stress of BGA solder joints based on the Co-Kriging multi-fidelity surrogate model (MFS) is proposed. The maximum residual stress after reflow soldering is obtained by simulating the melting forming and cooling solidification stages of the solder joints. The MFS between the size parameters such as the height of the solder joint and the radius of the pad and the maximum residual stress after reflow soldering is established. The meshes of different sizes are divided into two stages, and the nested and non-nested sampling methods are adopted to form four MFSs. By comparing the prediction effect of the MFS with the single-fidelity surrogate model, namely the radial basis function, the particle swarm optimization (PSO) algorithm is used to optimize the solder joint size parameters based on the prediction results of the MFS. The results show that the prediction effect of the MFS is better than that of the single-fidelity surrogate model; The best construction scheme of the MFS is: in the simulation stage of solder joint melting forming, mesh generation with different precision is carried out to obtain high/low precision sample points, and the sample points are not nested. The results of PSO prove the validity of the MFS and obtain the optimal solder joint parameters.},
  creationdate = {2025-08-08T17:31:59},
  keywords     = {BGA, Residual stress, Simulation prediction, Multi-fidelity surrogate model, Particle swarm optimization},
  owner        = {mamrani},
}


@Article{10.1007/s10040-022-02451-2,
  author       = {Di Zhang and Yun Yang and Jian Song and Jianfeng Wu and Xiaomin Sun and Jin Lin and Xiaobin Zhu and Jichun Wu},
  date         = {2022-03},
  journaltitle = {Hydrogeology Journal},
  title        = {Optimizing river damming and impounding strategies to mitigate seawater intrusion in the coastal aquifer of Dagu River Basin, China},
  doi          = {10.1007/s10040-022-02451-2},
  number       = {2},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s10040-022-02451-2},
  volume       = {30},
  abstract     = {Une retenue sur la rivière est une alternative à la recharge artificielle des eaux souterraines dans les aquifères côtiers, en ce qu’elle atténue l’intrusion de l’eau de mer causée par les activités humaines. La présente étude met en œuvre un nouveau cadre d’optimisation multiobjectifs en vue de la gestion des mesures d’ingénierie existantes concernant la retenue multibarrages de la rivière et des stratégies d’exploitation des eaux souterraines dans le Bassin de la Rivière Dagu, un bassin du Nord de la Chine affecté par une pénurie d’eau. Le cadre d’optimisation intègre un modèle numérique calibré avec un ensemble conceptualisé de rivières dans le but de simuler l’interaction entre La Rivière Dagu et les eaux souterraines attribuable à la retenue de la rivière; il se concentre sur le compromis entre la minimisation de l’étendue de l’intrusion d’eau salée, la minimisation des infiltrations de la rivière et la maximisation de l’exploitation admissible des eaux souterraines. L’algorithme génétique de tri élitiste non dominé (NSGA-II) est ensuite utilisé pour rechercher les solutions optimales de Pareto affectées par les différentes conditions climatiques. Les résultats montrent que le niveau de l’eau de la rivière et les schémas d’exploitation des eaux souterraines peuvent être ajustés de manière à réaliser les compromis optimaux entre les différents objectifs. Pour limiter l’intrusion de l’eau de mer, le niveau de l’eau de la rivière dans la retenue derrière la digue jouxtant l’intrusion d’eau salée associée aux années de précipitations insuffisantes, devrait être augmenté d’au moins 0.5–1.5 m, tandis que le niveau de la rivière derrière les digues restantes devrait être réduit d’au moins 1 m. Qui plus est, face à la contrainte de réduction des prélèvements dans les eaux souterraines, l’exploitation annuelle dans les quatre divisions administratives au rang de comtés devrait être réduite de 1.2–1.5 millions m^3 ou davantage dans les années recevant moins de précipitations par rapport à celle des années recevant plus de précipitations. Ces stratégies pourraient également être appliquées à d’autres aquifères côtiers soumis à des activités humaines intenses. River impoundment is an alternative to artificial groundwater recharge in coastal aquifers, while mitigating seawater intrusion caused by anthropogenic activities. This study implements a new multiobjective optimization framework for management of the existing engineering measures of river multidamming impoundment and strategies for groundwater abstraction in Dagu River Basin, a water-scarce basin in north China. The optimization framework integrates a calibrated numerical model with a conceptualized river package to simulate the interaction between the Dagu River and groundwater due to river impoundment, and it focuses on the tradeoff among minimizing the seawater intrusion extent, minimizing the river seepage, and maximizing the allowable groundwater abstraction. Then the elitist nondominated sorting genetic algorithm (NSGA-II) is used to search for the Pareto-optimal solutions affected by different climate conditions. Results show that the river water level and groundwater abstraction schemes can be regulated to achieve the optimal tradeoffs among different objectives. To mitigate seawater intrusion, the impoundment river water level behind the dam adjacent to the seawater intrusion associated with years of insufficient precipitation should be raised by at least 0.5–1.5 m, while the river level behind the remaining dams should be reduced by at least 1 m. Moreover, under the constraint of restricting groundwater abstraction, the annual groundwater abstraction in the four county-level administrative subdomains should be adjusted by 1.2–1.5 million m^3 or more in years with less precipitation compared to that in years with more precipitation. The implications might also be applied to other coastal aquifers with intense human activities. 河道蓄水是沿海含水层中人工补给地下水的一种可行选择, 同时可缓解人类活动造成的海水入侵。本研究针对中国北方大沽河流域水资源短缺的现状, 提出了一个新的多目标优化框架, 用于优化管理该流域的现有多级拦河闸坝蓄水工程措施和地下水开采策略。该优化框架将一业已校准的数值模型与另一概化的河流子程序包进行耦合, 以模拟因河流蓄水而产生的大沽河-地下水的水力联系, 其重点关注最小化海水入侵程度、最小化河流渗流和最大化地下水允许开采量等3个目标之间的权衡关系。然后, 采用精英非支配排序的遗传算法(NSGA-II)来搜索优化模型在不同气候条件影响下的帕累托最优解。结果表明, 可通过调节河流蓄水位和地下水开采方案来实现不同目标之间的最佳权衡。为了缓解海水入侵, 在降水不足的年份, 靠近海水入侵区的闸坝至少要将其蓄水位提高0.5–1.5 m, 而其他闸坝则需将其蓄水位至少降低1 m。此外, 在地下水压采约束条件下, 4个县级行政分区的地下水年开采量在降水较少的年份应比降水较多的年份浮动120–150万m^3甚至更多。本文研究启示亦可适用于人类活动强烈的其他沿海含水层。 Los embalses fluviales son una alternativa a la recarga artificial de las aguas subterráneas en los acuíferos costeros, al tiempo que mitigan la intrusión de agua de mar causada por las actividades antropogénicas. Este estudio implementa un nuevo marco de optimización multiobjetivo para la gestión de las medidas de ingeniería existentes de embalses fluviales y estrategias de extracción de aguas subterráneas en la cuenca del río Dagu, una cuenca con escasez de agua en el norte de China. El marco de optimización integra un modelo numérico calibrado con un paquete fluvial conceptualizado para simular la interacción entre el río Dagu y las aguas subterráneas debido al embalse fluvial, y se centra en el equilibrio entre la minimización de la extensión de la intrusión de agua de mar, la minimización de la filtración fluvial y la maximización de la extracción de agua subterránea permitida. A continuación, se utiliza el algoritmo genético de ordenación no dominante (NSGA-II) para buscar las soluciones óptimas de Pareto afectadas por diferentes condiciones climáticas. Los resultados muestran que el nivel de agua del río y los esquemas de extracción de agua subterránea pueden ser regulados para lograr los compromisos óptimos entre los diferentes objetivos. Para mitigar la intrusión de agua de mar, el nivel del agua del río embalsado detrás de la presa adyacente a la intrusión de agua de mar asociada a años de precipitaciones insuficientes debería elevarse al menos 0.5–1.5 m, mientras que el nivel del río detrás de las presas restantes debería reducirse al menos 1 m. Además, bajo la restricción de la extracción de aguas subterráneas, la extracción anual de aguas subterráneas en los cuatro subdominios administrativos a nivel de condado debería ajustarse en 1.2–1.5 millones de m^3 o más en los años con menos precipitaciones en comparación con los años con más precipitaciones. Las conclusiones podrían aplicarse también a otros acuíferos costeros con intensa actividad humana. O barramento de rios é uma alternativa à recarga artificial da água subterrânea em aquíferos costeiros, ao mesmo tempo que mitiga a intrusão da água do mar causada por atividades antrópicas. Este estudo implementa uma nova estrutura de otimização multiobjetivo para a gestão das medidas existentes de engenharia de represamento de rios e estratégias para captação de água subterrânea na Bacia do Rio Dagu, uma bacia com escassez de água no norte da China. A estrutura de otimização integra um modelo numérico calibrado com um pacote de rio conceituado para simular a interação entre o Rio Dagu e as águas subterrâneas devido ao represamento do rio, e se concentra na compensação entre minimizar a extensão da intrusão de água do mar, minimizar a infiltração do rio e maximizar captação de água subterrânea permitida. Em seguida, o elitista não dominado algoritmo genético de ordenação (NSGA-II) é usado para pesquisar as soluções ótimas de Pareto afetadas por diferentes condições climáticas. Os resultados mostram que o nível da água do rio e os esquemas de captação de água subterrânea podem ser regulados para atingir as compensações ideais entre os diferentes objetivos. Para mitigar a intrusão de água do mar, o nível de represamento do rio atrás da barragem adjacente à intrusão de água do mar associada a anos de precipitação insuficiente deve ser aumentado em pelo menos 0.5–1.5 m, enquanto o nível do rio atrás das barragens restantes deve ser reduzido em pelo menos 1 m. Além disso, sob a restrição de restringir a captação de água subterrânea, a captação anual de água subterrânea nos quatro subdomínios administrativos de nível municipal deve ser ajustada em 1.2–1.5 milhões de m^3 ou mais em anos com menos precipitação em comparação com anos com mais precipitação. As implicações também podem ser aplicadas a outros aquíferos costeiros com atividades humanas intensas.},
  creationdate = {2025-08-08T17:32:06},
  keywords     = {Seawater intrusion, Coastal aquifers, Multi-objective optimization, River damming impoundment, China},
  owner        = {mamrani},
  page         = {557–573},
  publisher    = {Springer},
}


@Article{10.1038/s41598-025-12887-7,
  author       = {Han Zhang and Maoyuan Zhang and Xin Li and Chunwei Song},
  date         = {2025-07},
  journaltitle = {Scientific Reports},
  title        = {Reserve optimization model of wind power with the coordination of multiple type electric power system sources},
  doi          = {10.1038/s41598-025-12887-7},
  number       = {1},
  url          = {https://www.nature.com/articles/s41598-025-12887-7.pdf},
  volume       = {15},
  abstract     = {Aiming at the optimization problem of frequency regulation energy reserve cost faced by wind power stations participating in primary frequency regulation, a reserve optimization model of wind power based on DPGMM-LSTM with the coordination of multi-type electric power system sources is proposed. The optimization is achieved through the coordinated response regulation of multi-energy reserve directly or indirectly connected by wind turbines or wind power stations, to realize the optimization of frequency regulation reserve costs under given frequency regulation response characteristics. First, the frequency regulation characteristics of wind turbines, energy storage systems, integrated energy loads and thermal power units are studied by establishing the wind power station system model with multi-frequency regulation reserve resources (MFRRR). The frequency regulation characteristics model of the MFRRR is established. Then, a reserve optimization strategy of wind power considering multi-type electric power system sources coordination is studied based on DPGMM-LSTM. Then, the frequency regulation reserve cost optimization model of MFRRR is established. The objective of the optimization model is to minimize the frequency regulation reserve cost of MFRRR. Subsequently, the model is solved by the combination of DPGMM and LSTM. Finally, a wind power station simulation model with MFRRR access is established to verify the effectiveness of the reserve optimization method proposed in this paper.},
  creationdate = {2025-08-08T17:32:08},
  keywords     = {Primary frequency regulation, Cost optimization, Frequency regulation reserve resource, Renewable energy, LSTM},
  owner        = {mamrani},
  page         = {1–20},
  publisher    = {Nature},
}


@Article{10.1007/s00366-021-01300-2,
  author       = {Lili Zhang and Jiexiang Hu and Xiangzheng Meng and Peng Jin},
  date         = {2022-06},
  journaltitle = {Engineering with Computers},
  title        = {An efficient optimization method for periodic lattice cellular structure design based on the K-fold SVR model},
  doi          = {10.1007/s00366-021-01300-2},
  number       = {2},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/s00366-021-01300-2},
  volume       = {38},
  abstract     = {The design optimization of periodic lattice cellular structure relying exclusively on the computational simulation model is a time-consuming, even computationally prohibitive process. To relieve the computational burden, an efficient optimization method for periodic lattice cellular structure design based on the K-fold support vector regression model (K-SVR) is proposed in this paper. First, based on the loading experiments, the most promising unit cell of periodic lattice cellular structure is selected from five typical unit cells. Second, an initial SVR model is constructed to replace the simulation model of the periodic lattice cellular structure, and the K-fold cross-validation approach is used to extract the error information from the SVR model at the sample points. According to the error information, the sample points are sorted and classified into several sub-sets. Then, a global K-SVR model is re-constructed by aggregating each SVR model under each sub-set. Third, considering that there exists prediction errors between the K-SVR model and the simulation model, which may lead to infeasible optimal solutions, an uncertainty quantification approach is developed to ensure the feasibility of the optimal solution for the periodic lattice cellular structure design. Finally, the effectiveness and merits of the proposed approach are demonstrated on the design optimization of the A-pillar and seat-bottom frame.},
  creationdate = {2025-08-08T17:32:11},
  keywords     = {Periodic lattice cellular structure, Support vector regression model, Additive manufacturing, Uncertainty quantification, Design optimization},
  owner        = {mamrani},
  page         = {1095–1109},
  publisher    = {Springer},
}


@InProceedings{10.1007/978-3-031-08530-7_18,
  author       = {Liwen Zhang and Hervé Pingaud and Elyes Lamine and Franck Fontanili and Christophe Bortolaso and Mustapha Derras},
  booktitle    = {Advances and Trends in Artificial Intelligence. Theory and Practices in Artificial Intelligence},
  date         = {2022-01},
  title        = {A Model Driven Approach to Transform Business Vision-Oriented Decision-Making Requirement into Solution-Oriented Optimization Model},
  doi          = {10.1007/978-3-031-08530-7_18},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-031-08530-7_18},
  abstract     = {Currently in our highly connected society, there is a strong requirement for decision-makers in organizations to coordinate and schedule their activities. Frequently, there are various uncertain factors, multiple objectives, many business knowledge and requirements, which heavily increase the difficulty of decision-making process regarding these issues. Therefore, a decision-maker will appreciate having control over the formulation of decision-making models and being able to adapt to highly dynamic situation. In this paper, we study a Model Driven Engineering (MDE) approach to link the business requirement defined by a model with solution-oriented logical models, which are codes that could be submitted to a combinatorial optimization solver. The design of our proposal follows the principles of three-levels Model Driven Architecture (MDA) and is based on a cognitive process for decision-making systems. Then, several transformation rules between models are explained to realize automatic Model to Model Transformation (M2M) with a special emphasis on the Platform Independent Model (PIM) to Platform Specific Model (PSM) part. To make a proof of our model transformation chain efficiency, a classical Travelling Salesman Problem (TSP) is chosen as a use case.},
  creationdate = {2025-08-08T17:32:12},
  keywords     = {Model driven architecture, Model to model transformation, Decision-making support, Scheduling problem},
  owner        = {mamrani},
}


@InProceedings{10.1007/978-981-19-4109-2_33,
  author       = {Qing Zhang and Hanhua Zou and Zhigao Zeng and Sanyou Zeng},
  booktitle    = {Exploration of Novel Intelligent Optimization Algorithms},
  date         = {2022-01},
  title        = {An Uncertainty Estimation Method of Support Vector Machine Surrogate Model Assisting for Expensive Optimization},
  doi          = {10.1007/978-981-19-4109-2_33},
  publisher    = {Springer},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-19-4109-2_33},
  abstract     = {In data-driven evolutionary optimization, surrogate model is usually used to replace expensive physical experiments or simulations to reduce the evaluation cost. When selecting promising individuals for exact evaluation, those with a large degree of uncertainty in approximation also need to be taken into account, which can strengthen the exploration ability of algorithms and improve the accuracy of surrogates. Support vector machine (SVM) is suitable for expensive small sample training set. However, it can only predict the mean value without the uncertainty. In order to solve this problem, the zero mean Gaussian random process is used to measure the uncertainty of support vector machine. The method’s efficiency is demonstrated on a set of benchmark problems. The results show that the uncertainty measurement method used in this paper improves the exploration ability of the evolutionary algorithm and obtains a better solution.},
  creationdate = {2025-08-08T17:32:13},
  keywords     = {Evolutionary optimization, Surrogate model, SVM, Gaussian process},
  owner        = {mamrani},
}


