@Article{10.1007/s10479-021-04119-8,
  author       = {Bo wang and Nana Deng and Wenhui Zhao and Zhaohua Wang},
  date         = {2022-09},
  journaltitle = {Annals of Operations Research},
  title        = {Residential power demand side management optimization based on fine-grained mixed frequency data},
