@InProceedings{10.1007/978-3-031-39698-4_37,
  author       = {Tiago Trevisan Jost and Arun Thangamani and Raphaël Colin and Vincent Loechner and Stéphane Genaud and Bérenger Bramas},
  booktitle    = {Euro-Par 2023: Parallel Processing},
  date         = {2023-01},
