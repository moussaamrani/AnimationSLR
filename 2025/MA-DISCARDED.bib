@Article{10.1007/s00117-024-01393-y,
  author       = {Juan Du and Linsha Yang and Tao Zheng and Defeng Liu},
  date         = {2024-11},
  journaltitle = {Die Radiologie},
  title        = {Radiomics-based predictive model for preoperative risk classification of gastrointestinal stromal tumors using multiparametric magnetic resonance imaging: a retrospective study},
