@InProceedings{10.1007/978-3-031-73741-1_19,
  author       = {Maike Schwammberger},
  booktitle    = {Bridging the Gap Between AI and Reality},
  date         = {2025-01},
  title        = {From Explanation Correctness to Explanation Goodness: Only Provably Correct Explanations Can Save the World},
