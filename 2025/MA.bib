  author    = {Runjuan Kong and Wei Li and Haigang Wang and Qiangqiang Ren},
  journal   = {Journal of Thermal Science},
  title     = {Energy Efficiency Analysis of Oxy-Fuel Circulating Fluidized Bed Combustion System with High Oxygen Concentration},
