}


@InProceedings{10.1007/978-981-16-7469-3_15,
  author    = {Xin Liu and Lianyue Zheng and Yue Yang and Xinyu Zhao},
  booktitle = {2021 International Conference on Big Data Analytics for Cyber-Physical System in Smart City},
  title     = {Construction Safety Early Warning System of Power Transmission and Transformation Project Based on GIM},
  year      = {2022},
  month     = {jan},
  publisher = {Springer},
  abstract  = {In the digital era, the construction of smart power grid puts forward higher requirements for engineering construction. In order to improve the level of security control and work efficiency of power transmission and transformation construction site, an intelligent early warning system based on video recognition and personnel positioning is developed, verified and applicated in this paper. The functions of timely collection of safety risks, accurate judgement of safety rules and real-time feedback of early warning results are realized. At the same time, through the positioning subsystem server and the on-site intelligent security behavior analysis server, the warning notice is converted into digital signal and linked with the on-site voice broadcast equipment to realize the on-site warning, informing and reminding function.},
  doi       = {10.1007/978-981-16-7469-3_15},
  keywords  = {Power transmission and transformation engineering, Intelligence security early warning, Video identity, Personnel positioning, GIM},
  url       = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-16-7469-3_15},
}


@InProceedings{10.1007/978-3-031-13841-6_4,
  author    = {Ying Qu and Xiansheng Yang and Yixin Xie and Yunjiang Lou},
  booktitle = {Intelligent Robotics and Applications},
  title     = {Model Predictive 6D Image-Based Visual Servoing for 3C Products Assembly},
  year      = {2022},
  month     = {jan},
  publisher = {Springer},
  abstract  = {The application of visual servoing in robot assembly is very promising. In this paper, the 6-degree-of-freedom (DOF) image-based visual servoing (IBVS) control based on model predictive control (MPC) is implemented for 3C products assembly. Since the classical IBVS is calculated by multiplying the error times the pseudoinverse of an interaction matrix, it cannot handle constraints. Hence, the collision encountered in the process and the feature points out of the visual range cannot be solved well. Therefore, based on MPC, which can solve the servoing problems by transforming them into constrained optimization, this paper proposes a new MPC for the 3C products assembly system. In the new MPC, we estimate the depth information through image information, to solve the problem of deep collision, and propose a new cost function, which can better solve the problem of the system falling into a local optimal solution. Finally, the simulation verified in the ROS-Gazebo simulation results shows that the new control method is feasible and the control performance is more satisfactory.},
  doi       = {10.1007/978-3-031-13841-6_4},
  keywords  = {Image-based visual servoing (IBVS), Model predictive control (MPC), Assembly, Robot control},
  url       = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-031-13841-6_4},
}


@InProceedings{10.1007/978-981-19-6203-5_31,
  author    = {Mingxiu Han and Haihong Wang},
  booktitle = {Proceedings of 2022 Chinese Intelligent Systems Conference},
  title     = {The Research on Fuzzy Immune Method for Nonlinear Systems with Disturbance Suppression Based on U-model},
  year      = {2022},
  month     = {jan},
  publisher = {Springer},
  abstract  = {A class of nonlinear systems with unknown continuous disturbances control problems are investigated in this paper. In order to simplify the calculation, the original nonlinear system is firstly transformed into a polynomial system based on U-model. To compensate disturbances, an improved active disturbance rejection control (ADRC) is introduced by combining the Fuzzy immune (FI). FI replaces the nonlinear error feedback (NLSEF) to improve the adaptive ability in suppressing disturbance, and the FI active disturbance rejection control based on U-model (FI-UADRC) is designed to realize a tracking control with unknown continuous disturbances. The final simulations show that FI-UADRC simplifies the parameter adjustment and maintains good anti-disturbance performance of UADRC, the tracking speed and accuracy under continuous unknown disturbance are better than UADRC.},
  doi       = {10.1007/978-981-19-6203-5_31},
  keywords  = {U-model, Fuzzy immune, Disturbance suppression},
  url       = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-19-6203-5_31},
}


@InProceedings{10.1007/978-3-031-11321-5_4,
  author    = {Aziz Sfar and Dina Irofti and Madalina Croitoru},
  booktitle = {Foundations of Information and Knowledge Systems},
  title     = {A Graph Based Semantics for Logical Functional Diagrams in Power Plant Controllers},
  year      = {2022},
  month     = {jan},
  publisher = {Springer},
  abstract  = {In this paper we place ourselves in the setting of formal representation of functional specifications given in logical diagrams ( LD ) for verification and test purposes. Our contribution consists in defining a formal structure that explicitly encodes the semantics and behavior of a LD . We put in a complete transformation procedure of the non-formal LD specifications into a directed state graph such that properties like oscillatory behavior become formally verifiable on LDs . We motivate and illustrate our approach with a scenario inspired from a real world power plant specification.},
  doi       = {10.1007/978-3-031-11321-5_4},
  keywords  = {System Validation, Functional Specifications, Logic Functional Diagram, Graph based Knowledge Representation and Reasoning},
  url       = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-031-11321-5_4},
}


@InProceedings{10.1007/978-3-030-99310-8_14,
  author    = {Jan Duda and Sylwester Oleszek and Krzysztof Santarek},
  booktitle = {Advances in Manufacturing III},
  title     = {Product Lifecycle Management (PLM) in the Context of Industry 4.0},
  year      = {2022},
  month     = {jan},
  publisher = {Springer},
  abstract  = {The article presents the essence, genesis and development trends of PLM systems. Currently, these are advanced tools supporting the work of engineers at all stages of the product lifecycle. They are particularly useful, and even necessary, in the design of products integrating various fields of technology (engineering), i.e. products with mechanical, electrical, electronic, IT components, etc. The development of Industry 4.0 technology creates new requirements, but also opportunities for PLM systems. The article presents the essence, genesis and development trends of PLM systems. I also presents the selected examples of the use of Industry 4.0 technology in PLM systems, including the industrial Internet of Things, virtual and augmented reality and digital twins.},
  doi       = {10.1007/978-3-030-99310-8_14},
  keywords  = {Product Lifecycle Management (PLM), Application Lifecycle Management, Service Lifecycle Management, Connected PLM, Industry 4.0},
  url       = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-030-99310-8_14},
}


@InProceedings{10.1007/978-3-030-96648-5_11,
  author    = {John Grundy and Ingo Mueller and Anuradha Madugalla and Hourieh Khalajzadeh and Humphrey O. Obie and Jennifer McIntosh and Tanjila Kanij},
  booktitle = {Evaluation of Novel Approaches to Software Engineering},
  title     = {Addressing the Influence of End User Human Aspects on Software Engineering},
  year      = {2022},
  month     = {jan},
  publisher = {Springer},
  abstract  = {We outline some of the key challenges in supporting diverse human aspects of end users in software engineering. This includes examples of age, gender, physical and mental challenges, human values, personality, emotions, language and culture. We review key related work from a range of disciplines, and propose an initial taxonomy of end user human aspects that need careful consideration throughout the software engineering life cycle. Finally we outline a research road map of key areas requiring further investigation and work.},
  doi       = {10.1007/978-3-030-96648-5_11},
  keywords  = {Human aspects of end users, Taxonomy, Living lab, Stakeholders, Personas, Modelling, Design, Defect reporting},
