@InProceedings{10.1007/978-981-97-9910-7_1,
  author       = {Khuat Duc Anh and Vu Kim Anh and Nguyen Duy Minh and Luong Nguyen Viet Khoa and Ngo Truong Minh and Phan Duy Hung},
  booktitle    = {Kansei Engineering and Emotion Research},
  title        = {SmartTM: An IoT-Based Smart Trash Management System},
  year         = {2024},
  month        = {jan},
  publisher    = {Springer},
  abstract     = {The rapid expansion of the global population, along with advancements in modern technology brings many benefits such as convenient living, but it also exacerbates the issue of waste management, a growing environmental concern. As the amount of solid waste increases, it places additional strain on the waste management system and infrastructure. Especially in some healthcare circumstances, trash bins overflow are mismanaged and hazardous waste spreads around the area. This can cause injury, illness or even death to people and animals that are exposed. Addressing the above challenges, this paper introduces a smart waste management system for urban environments, utilize technology to optimize garbage handling. Sensors inside trash bins relay data to a central processing unit, which then can be used to analyze for further important information. Additionally, the system also contains a web application to present collected data, assist managers and staff to monitor trash bin statuses by time, facilitating more informed decision-making. Overall, the SmartTM system allows data-driven collection scheduling, thus eliminating unnecessary trips to empty bins, and disposing of full-filled bins on time. It results in fuel savings, and ensures environmental cleanliness by preventing overflow of normal waste, solid waste or hazardous waste. Also, the novelty of the project is the absence of a GPS module, which makes the device more portable and minimizes power consumption. This paper assembled three different hardware prototypes, including investigation reports of battery lifetime and LoRa distance testing of the end node and a web application for data visualization.},
  creationdate = {2025-08-07T18:21:51},
  doi          = {10.1007/978-981-97-9910-7_1},
  keywords     = {Iot monitoring, smart waste management, solid waste, LoRaWAN, distance measurement sensors, environment cleanliness},
  owner        = {mamrani},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-981-97-9910-7_1},
}


@InProceedings{10.1007/978-3-031-93706-4_6,
  author       = {Calvin Beck and Hanxi Chen and Steve Zdancewic},
  booktitle    = {NASA Formal Methods},
  title        = {Vellvm: Formalizing the Informal LLVM},
  year         = {2025},
  month        = {jan},
  publisher    = {Springer},
  abstract     = {This report presents our methodology for and experience with formalizing a specification of LLVM IR in the Verified LLVM Project (Vellvm). Vellvm provides a specification for a large, practical subset of LLVM IR in the Rocq Proof Assistant in the support of verified compilers and program transformations. Program transformations often rely on the subtle details, and as a result Vellvm’s semantics are quite comprehensive: for instance we provide a sophisticated low-level memory model to support low-level operations, such as casts between pointers and integers, and justify optimizations in their presence. Our approach implements the semantics via monadic interpreters , which rely on a coinductively-defined data structure called ITrees . Crucially, this methodology supports the extraction of an executable interpreter, proved to refine the specification. We use the reference interpreter to validate the accuracy of the formalization, employing random differential testing between Clang and Vellvm implemented in our own LLVM IR program generator (GenLLVM), as well as via state-of-the-art C compiler testing frameworks (CSmith and YARPGen). Such testing has found bugs in both the Vellvm semantics and Clang. We believe that tools from the Vellvm project can be useful for other LLVM IR-related projects, and that the overall methodology applies to other formal verification efforts.},
  creationdate = {2025-08-07T18:21:59},
  doi          = {10.1007/978-3-031-93706-4_6},
  keywords     = {Rocq, LLVM, Semantics},
  owner        = {mamrani},
  url          = {http://link.springer.com/openurl/pdf?id=doi:10.1007/978-3-031-93706-4_6},
}

